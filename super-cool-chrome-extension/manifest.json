{
  "manifest_version": 3,
  "name": "Super Cool Extension",
  "version": "0.1",
  "web_accessible_resources": [
    {
      "resources": ["kittens/*.jpg"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "fox.png"
  }
}
